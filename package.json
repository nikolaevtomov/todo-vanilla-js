{
  "name": "todo-vanilla-js",
  "version": "0.0.1",
  "description": "a better typescript project",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG=true webpack serve --mode development",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:js": "cross-env NODE_ENV=production DEBUG=false webpack --mode production --progress",
    "build:all": "npm run build:clean && npm run build:js",
    "build": "npm run build:all",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx,css,scss,less,json}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit && eslint 'src/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-const-enum": "^1.0.1",
    "core-js": "^3.10.1",
    "normalize-scss": "^7.0.1",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^7.0.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:nikolaevtomov/todo-vanilla-js.git"
  }
}
